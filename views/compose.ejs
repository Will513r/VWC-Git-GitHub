<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
<div class="form-group">
  <label for="blogTitle" class="form-label">Title</label>
  <input type="text" class="form-control" name="blogTitle" >

  <label for="blogPost" class="form-label">Post</label>
  <textarea class="form-control" name="blogPost" rows="5" cols="30" ></textarea>
</div>
<button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>



<%- include("partials/footer") -%>